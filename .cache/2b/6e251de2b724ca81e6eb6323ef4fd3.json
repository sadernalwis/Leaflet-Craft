s.attachEvents(map);\n  }\n\n  attachEvents(map) {\n    this.leafletElement.on('markers', e => {\n      if (e.eventType === 'create') {\n        clickUndo(map);\n        clickRedo(map);\n      }\n    });\n    // this.leafletElement.on('mode', this.props.onModeChange);\n  }\n\n  componentWillUnmount() {\n    // this.leafletElement.mode(0);\n    const { map } = this.props.leaflet;\n    map.removeLayer(this.leafletElement);\n  }\n\n  render() {\n    return null;\n  }\n}\n\nexport default withLeaflet(ReactFreeCraft);"},"lineCount":null}},"error":null,"hash":"cc2055afdeabceb903187f5d4ddde9e0","cacheData":{"env":{}}}